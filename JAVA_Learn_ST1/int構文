int構文
public class Main {
	public static void main(String[] args) {
		System.out.println("あなたの運勢を占います");
    	int fort = 1;
    	switch (fort) {
	        case 1:
	            System.out.println("大吉");
            break;
	        case 2:
	            System.out.println("中期");
	        break;
	        case 3:
	            System.out.println("小吉");
	        break;
	        default:
	            System.out.println("狂");
    	}

	}
}

